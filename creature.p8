  stime:from(currenttime())
--  printh("save:"..stime:tostring())
  stime:save()
 else
  stime:read()
--  printh("read:"..stime:tostring())
--  printh("starttime = ".. )
 end

 cls()
-- printh("goaltime"..goaltime)
 m:ctmap()

 m:draw()
 ui:draw()
end

