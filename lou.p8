function lerp(x,y,t)
 return x+(y-x)*t
end

function async(func)
 add(routines,cocreate(func))
end

function fadeout()
	for i=0,32 do
		fillp(‚ñí)
		circfill(64,64,
		lerp(128*.75,0,i/32),0|0x1800)
		fillp()
		circfill(64,64,
		lerp(128,0,i/32),0|0x1800)
		yield()
	end
	cs=ns
	cls()
end


function fadein()
 cls()
	for i=0,32 do
		fillp(‚ñí)
		circfill(64,64,
		lerp(0,128*.75,i/32),0|0x1800)
		fillp()
		circfill(64,64,
		lerp(0,128,i/32),0|0x1800)
		yield()
	end
end


