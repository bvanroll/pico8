	    srand(x*y)
	    rectfill(x,y,x,y,rnd()*16)
	   end
	  end
	  spr(sprites.lou.order[1],
	   16+self.tx*8,
	   16+self.ty*8,
	   sprites.lou.w,
	   sprites.lou.h)
	  spr(sprites.sam.order[1],
	   16+self.px*8,
	   16+self.py*8,
	   sprites.sam.w,
	   sprites.sam.h)
	  poke(0x5f34,0x2)
	  fillp(‚ñë)
	  circfill(24+self.px*8,
	  24+self.py*8,
	  20+sin(time()/2)*3,0|0x1800)
	  fillp(0)
	  circfill(24+self.px*8,
	  24+self.py*8,
	  28+cos(time()/4)*5,0|0x1800)
	  
	  
	 end,
	 score=function(self)
	  lou.excitement+=1
	 end
